Modal\";\nimport { Categories } from \"../../api/categories\";\nimport { Products } from \"../../api/products\";\nimport EditProductModal from \"./EditProductModal\";\n\nexport default {\n    name: \"ProductCard\",\n    props: ['product'],\n    components: {\n        EditProductModal,\n        ProductDetailsModal\n    },\n    data() {\n        return {\n            categoryName: '',\n            productId: this.product._id\n        };\n    },\n    mounted() {\n        this.findCategoryNameById();\n    },\n    meteor: {\n        $subscribe: {\n            products: [],\n            categories: []\n        },\n    },\n    methods: {\n        findCategoryNameById () {\n            let category = Categories.findOne({\n                \"_id\" : this.product.categoryId\n            });\n\n            this.categoryName = category.name;\n        },\n        deleteProduct(id){\n            swal({\n                title: \"Are you sure want to delete \"+ this.product.name +\"?\",\n                text: \"Once deleted, you will not be able to recover this product!\",\n                icon: \"warning\",\n                buttons: true,\n                dangerMode: true,\n            })\n            .then((willDelete) => {\n                if (willDelete) {\n                    let deleteProduct = Products.remove({\n                        \"_id\" : this.productId\n                    });\n                    swal(\"Poof! Your \"+ this.product.name +\" has been deleted!\", {\n                        icon: \"success\",\n                    });\n                } else {\n                    swal(\"Your \"+ this.product.name +\" is safe!\");\n                }\n            });\n        }\n    }\n}\n"]},"sourceType":"module","hash":"4d2e85bc66433baaa1db1b63cb5040eac1732bd2"}
